{
  "devDependencies": {
    "cypress": "^14.3.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
    
  },
  "scripts": {
  "merge-report": "npx mochawesome-merge cypress/reports/mochawesome/*.json -o cypress/reports/mochawesome/merged-report.json",
  "generate-report": "npx marge cypress/reports/mochawesome/merged-report.json -f mochawesome-report -o cypress/reports/mochawesome/html"
  }
  

}
